  object ZakazQuery: TpFIBDataSet
    SelectSQL.Strings = (
      
        'select cast(t.item as integer) as nitem, t.item, t.artikul, t.kk' +
        'm_sect, t.producer, t.remark, pr.sale_price, t.tovar_name, pr.ve' +
        'nd_price, v.vendor_name, u.unit_name, tovar_add.sclad, tovar_add' +
        '.mark_color, tovar_add.sale_mark_color, g.group_name, t.remark_1' +
        ','
      'n.norm_ost NORM_O, n.min_ost MIN_O, z.kolvo ZAK, SUM(o.kolvo)OST'
      
        ',(select sum(kolvo)from pos_rashod p1 right join hdr_rashod h1 o' +
        'n p1.kod = h1.kod where item = t.item and h1.doc_type = 12) RSRV'
      
        ',(select sum(kolvo) from tovar_ost where item = t.item and marke' +
        't_code = 101)OST101,'
      
        '(select sum(kolvo) from zakaz where item = t.item and market_cod' +
        'e = 101)ZAK101'
      
        'from spr_tovar t left join spr_vendors v on t.vendor_code = v.ve' +
        'ndor_code'
      'left join spr_units u on t.unit_kod = u.unit_kod'
      
        'left join spr_group g on t.t_group = g.t_group left join tovar_a' +
        'dd on tovar_add.item = t.item'
      
        'left join spr_norm_ost n on n.item = t.item and n.market_code = ' +
        '202'
      'left join zakaz z on z.item = t.item and z.market_code = 202'
      
        'left join prices pr on pr.item = t.item and pr.market_code = (se' +
        'lect OBJ_ID from INF_CURRENT_OBJ)'
      'left join tovar_ost o on o.item = t.item and o.market_code = 202'
      ''
      
        'group by t.item, t.artikul, t.kkm_sect, t.producer, t.remark_1, ' +
        't.remark, pr.sale_price, t.tovar_name, pr.vend_price, v.vendor_n' +
        'ame, u.unit_name, tovar_add.sclad, tovar_add.mark_color, tovar_a' +
        'dd.sale_mark_color, g.group_name, n.norm_ost, n.min_ost, z.kolvo'
      ''
      'ORDER BY 1')
    AfterOpen = ZakazQueryAfterOpen
    OnCalcFields = ZakazQueryCalcFields
    Transaction = ShopMainForm.pFIBTransaction1
    Database = ShopMainForm.pFIBDatabase1
    Filtered = True
